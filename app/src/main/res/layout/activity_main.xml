<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/plainTextEanView"
        android:layout_width="383dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/leerText"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewEan"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="81dp" />

    <Button
        android:id="@+id/buttonSpeichern"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/buttonSpeichern"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="532dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewMenge"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="416dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewLagerort"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="475dp" />

    <EditText
        android:id="@+id/plainTextLagerortView"
        android:layout_width="278dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/leerText"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="469dp" />

    <Spinner
        android:id="@+id/spinnerMenueSpinner"
        android:layout_width="138dp"
        android:layout_height="41dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="675dp" />

    <Spinner
        android:id="@+id/spinner7"
        android:layout_width="138dp"
        android:layout_height="41dp"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="675dp" />

    <EditText
        android:id="@+id/plainTextMengeView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/leerText"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="403dp" />

    <ImageButton
        android:id="@+id/imageButtonTastatur"
        android:layout_width="138dp"
        android:layout_height="41dp"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="674dp"
        tools:srcCompat="@tools:sample/backgrounds/scenic" />

    <TextView
        android:id="@+id/textViewEanNichtGefundenView"
        android:layout_width="381dp"
        android:layout_height="31dp"
        android:text="@string/leerText"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="175dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewStamm"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="538dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewEingabe"
        tools:layout_editor_absoluteX="282dp"
        tools:layout_editor_absoluteY="538dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewStammZahl"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="563dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewEingabeZahl"
        tools:layout_editor_absoluteX="303dp"
        tools:layout_editor_absoluteY="563dp" />

</androidx.constraintlayout.widget.ConstraintLayout>